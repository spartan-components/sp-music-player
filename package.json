{
  "name": "@spartan-components/sp-music-player",
  "version": "0.1.1",
  "description": "Webcomponent sp-music-player following open-wc recommendations",
  "author": "sp-music-player",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "web-dev-server",
    "lint": "eslint --ext .js,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch"
  },
  "dependencies": {
    "lit-element": "^2.5.1",
    "lit-html": "^1.4.1"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^2.5.33",
    "@web/dev-server": "^0.1.17",
    "@web/test-runner": "^0.12.20",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartan-components/sp-music-player.git"
  },
  "keywords": [
    "Web",
    "Components",
    "Custom",
    "Elements"
  ],
  "bugs": {
    "url": "https://github.com/spartan-components/sp-music-player/issues"
  },
  "homepage": "https://github.com/spartan-components/sp-music-player#readme"
}
